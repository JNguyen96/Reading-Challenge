			//h4 Post-Grad Sisters: <span id='postgradS'></span>
			
			// /BAR GRAPH

		//CHART WRAPPER
		#chartWrapper
			//CFC
			#cfcList
				h2 Case for Christmas
				table
					thead
						th Read Case for Christmas
					tbody
			// /CFC


			//SVM
			#svmList
				h2 Students vs Mike Kang
				table
					thead
						th Average books read by students
